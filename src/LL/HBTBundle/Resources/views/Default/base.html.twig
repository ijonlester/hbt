<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Household Budget Tracker{% endblock %}</title>
        {% block stylesheets %}
        <link href="{{ asset('bundles/llhbt/css/pepper-grinder/jquery-ui-1.10.1.custom.min.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('bundles/llhbt/css/main.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
        <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.1/jquery-ui.min.js"></script>
        <script type="text/javascript" src="https://www.google.com/jsapi"></script>
        <script src="{{ asset('bundles/llhbt/js/main.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/llhbt/js/chart.js') }}" type="text/javascript"></script>
        {% endblock %}
        <div class="error"></div>
    </body>
</html>
